   "source": [
    "# script to rename all text file to its folder name and move it outside\n",
    "import os"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 25,
   "metadata": {},
   "outputs": [],
   "source": [
    "texts = os.listdir(\"db/\")\n",
    "for text in texts:\n",
    "    files = os.listdir(\"db/\"+text)\n",
    "    for file in files:\n",
    "        os.rename(\"db/\"+text+'/'+file, text+'.txt')\n",
    "        print file"
   ]
  }
